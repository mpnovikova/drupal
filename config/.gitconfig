  email = mnovikova@ngpvan.com
  name = Marina Novikova
  signingkey = aegir_dev_blackmesh
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow
  frag = magenta
  old = red
  new = green
  whitespace = red reverse
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
  autocrlf = input
[alias]
  changes = log --name-status HEAD..origin/socialorg

